{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/execute-query",
      "dest": "/server.js"
    },
    {
      "src": "/test-connection",
      "dest": "/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/game",
      "dest": "/server.js"
    },
    {
      "src": "/mode/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/level/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/blog(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/setup",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
